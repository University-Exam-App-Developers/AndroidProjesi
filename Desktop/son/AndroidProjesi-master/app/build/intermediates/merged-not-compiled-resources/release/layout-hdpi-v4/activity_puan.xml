                    android:background="#ac3f21"
                    android:gravity="center"
                    android:hint="0"
                    android:inputType="numberDecimal"
                    android:textColor="#FFFFFF"
                    android:textColorHint="#FFFFFF" />

                <TextView
                    android:id="@+id/dinAyt_net"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#ac3f21"
                    android:gravity="center"
                    android:textColor="#FFFFFF" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_margin="2sp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/yabanciDil_AytPuan"
                    android:layout_width="160sp"
                    android:layout_height="match_parent"
                    android:background="#2f89fc"
                    android:padding="8dp"
                    android:text="Yabancı Dil (80)"
                    android:textColor="#FFFFFF" />

                <EditText
                    android:id="@+id/yabanciDil_dogru_EditText"
                    android:layout_width="50sp"
                    android:layout_height="match_parent"
                    android:background="#2f89fc"
                    android:gravity="center"
                    android:hint="0"
                    android:inputType="numberDecimal"
                    android:textColor="#FFFFFF"
                    android:textColorHint="#FFFFFF" />

                <EditText
                    android:id="@+id/yabanciDil_yanlis_EditText"
                    android:layout_width="50sp"
                    android:layout_height="match_parent"
                    android:background="#2f89fc"
                    android:gravity="center"
                    android:hint="0"
                    android:inputType="numberDecimal"
                    android:textColor="#FFFFFF"
                    android:textColorHint="#FFFFFF" />

                <TextView
                    android:id="@+id/yabanciDilAyt_net"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#2f89fc"
                    android:gravity="center"
                    android:textColor="#FFFFFF" />
            </LinearLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="23dp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/hesapla"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_toStartOf="@+id/middle"
                    android:background="@drawable/calculator2" />

                <Space
                    android:id="@+id/middle"
                    android:layout_width="50dp"
                    android:layout_height="10dp"
                    android:layout_centerInParent="true" />

                <Button
                    android:id="@+id/sifirla"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/middle"
                    android:background="@drawable/reset1" />

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>