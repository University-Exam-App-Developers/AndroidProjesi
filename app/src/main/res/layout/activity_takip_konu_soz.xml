<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/aytKonuSoz_edebiyat"
            android:text="EDEBİYAT"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
        <Button
            android:id="@+id/aytKonuSoz_tarih"
            android:text="TARİH"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
        <Button
            android:id="@+id/aytKonuSoz_cografya"
            android:text="COĞRAFYA"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
        <Button
            android:id="@+id/aytKonuSoz_felsefe"
            android:text="FELSEFE"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
        <Button
            android:id="@+id/aytKonuSoz_din"
            android:text="DİN KÜLTÜRÜ"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>